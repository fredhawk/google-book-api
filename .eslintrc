{
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["react-hooks", "prettier"],
  "root": true,
  "env": {
    "node": true,
    "browser": false,
    "es6": true
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react/jsx-one-expression-per-line": "off",
    "prettier/prettier": "error"
  }
}
